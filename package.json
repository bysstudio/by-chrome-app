{
  "name": "by-app",
  "version": "1.0.0",
  "description": "by",
  "main": "./out/main/index.js",
  "author": "biying.com",
  "homepage": "https://github.com/bysstudio/by-chrome-app",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "chcp 65001 && electron-vite dev",
    "watch": "electron-vite dev --watch",
    "build": "electron-vite build",
    "rebuild:addon": "npm run build:addon && electron-builder install-app-deps",
    "build:addon": "node-gyp rebuild --release --arch=x64 --target=34.3.0 --directory=window-api --dist-url=https://electronjs.org/headers",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --config.win.target=nsis",
    "build:portable": "npm run build && electron-builder --win --config.win.target=portable",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.9.4",
    "express": "^4.21.2",
    "geoip-lite": "^1.4.10",
    "https-proxy-agent": "^7.0.6",
    "knex": "^3.0.1",
    "pinia": "^3.0.1",
    "proxy-chain": "^2.5.6",
    "puppeteer": "^24.3.0",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "socks": "^2.8.4",
    "socks-proxy-agent": "^8.0.5",
    "sqlite3": "5.1.6",
    "uiohook-napi": "^1.5.4",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "34.3.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.1",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.1.0",
    "vue": "^3.5.13"
  }
}
